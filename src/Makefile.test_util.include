EXTRA_LIBRARIES += \
    $(LIBTEST_UTIL)

TEST_UTIL_H = \
    test/util/blockfilter.h \
    test/util/logging.h \
    test/util/mining.h \
    test/util/net.h \
    test/util/setup_common.h \
    test/util/str.h \
